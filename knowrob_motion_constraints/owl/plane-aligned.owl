<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY knowrob "http://ias.cs.tum.edu/kb/knowrob.owl#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY constr "http://ias.cs.tum.edu/kb/motion-constraints.owl#" >
]>


<rdf:RDF xmlns="http://ias.cs.tum.edu/kb/plane-aligned.owl#"
     xml:base="http://ias.cs.tum.edu/kb/plane-aligned.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:knowrob="http://ias.cs.tum.edu/kb/knowrob.owl#">
    <owl:Ontology rdf:about="http://ias.cs.tum.edu/kb/plane-aligned.owl">
        <owl:imports rdf:resource="package://knowrob_motion_constraints/owl/motion-constraints.owl"/>
    </owl:Ontology>


<!-- # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # -->

    <owl:Class rdf:about="http://ias.cs.tum.edu/kb/motion-constraints.owl#PlaneAligned">
        <rdfs:subClassOf rdf:resource="&knowrob;ITaSCMotion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&knowrob;constrainedBy"/>
                <owl:someValuesFrom rdf:resource="&constr;PerpendicularityConstraint_P4r7Zj4K"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


    <owl:Class rdf:about="&constr;PerpendicularityConstraint_P4r7Zj4K">
        <rdfs:subClassOf rdf:resource="&constr;PerpendicularityConstraint"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&constr;toolFeature"/>
                <owl:someValuesFrom rdf:resource="&knowrob;CenterOfObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&constr;worldFeature"/>
                <owl:someValuesFrom rdf:resource="&knowrob;CenterOfObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&constr;constrLowerLimit"/>
                <owl:hasValue rdf:datatype="&xsd;double">1.1</owl:hasValue> <!-- TODO: check this value -->
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&constr;constrUpperLimit"/>
                <owl:hasValue rdf:datatype="&xsd;double">1.3</owl:hasValue> <!-- TODO: check this value -->
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>



